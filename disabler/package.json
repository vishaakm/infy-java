{
  "name": "@vishaakm/disabler",
  "version": "1.0.0",
  "description": "This Extension disables the download option from the theia_ide",
  "keywords": [
    "theia-extension",
    "disable",
    "download disabler"
  ],
  "displayName": "Download disabler",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "latest"
  },
  "devDependencies": {
    "rimraf": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/disabler-frontend-module"
    }
  ],
  "author": "vishaak.m",
  "license": "ISC"
}